{
  "name": "stackoverflow-static",
  "description": "Scrape a static copy of your own StackOverflow votes data",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "rpc/rpc-storage.js",
      "rpc/rpc-background-init.js",
      "backend/init-common.js",
      "init.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "https://stackoverflow.com/*",
    "https://data.stackexchange.com/*"
  ],
  "web_accessible_resources": [
    "web-page/*",
    "rpc/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://stackoverflow.com/*",
      "https://data.stackexchange.com/*"
    ]
  },
  "page_action": {
    "default_popup": "backend/execute.html"
  },
  "options_page": "backend/options.html"
}
