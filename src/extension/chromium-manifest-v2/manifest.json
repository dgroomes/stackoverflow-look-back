{
  "name": "stackoverflow-static",
  "description": "Scrape a static copy of your own StackOverflow votes data",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "rpc/RpcServer.js",
      "rpc/ChromiumBackgroundRpcServer.js",
      "extension/common/init-common.js",
      "init.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "https://stackoverflow.com/*",
    "https://data.stackexchange.com/*"
  ],
  "web_accessible_resources": [
    "web/*",
    "rpc/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://stackoverflow.com/*",
      "https://data.stackexchange.com/*"
    ]
  },
  "page_action": {
    "default_popup": "extension/common/execute.html"
  },
  "options_page": "extension/common/options.html"
}
